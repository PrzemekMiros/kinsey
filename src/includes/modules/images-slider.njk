<section class="section section-slider-images pb-xlarge">
  <div class="container">
    <div class="slider slider-images js-slider-images text-center">
      <div class="slider-images__slider swiper-container js-slider-images__slider" 
      data-autoplay-enabled="true"
        data-autoplay-delay="6000" 
        data-centered-slides="false" 
        data-keyboard-enabled="true" 
        data-touch-ratio="1">
        <div class="swiper-wrapper">
          {% for post in collections.realizacje %}
          <!-- item -->
          <div class="swiper-slide slider-images__slide overflow">
            <div class="mask-reveal js-arts-parallax overflow" data-arts-parallax-factor-y="0.05"
              data-arts-os-animation="true" data-arts-os-animation-name="animateMask"
              data-arts-os-animation-params="{direction: 'up'}">
              <div class="mask-reveal__layer mask-reveal__layer-1">
                <div class="mask-reveal__layer mask-reveal__layer-2">
                  <div class="js-arts-parallax__scrub w-100 h-100">
                    <img class="swiper-lazy" src="#" data-src="{{ post.data.imageMain }}" width="1120" height="670"
                      alt="" />
                  </div>
                </div>
              </div>
            </div>
          </div>
          {% endfor %}
        </div>
        <div class="is-dragging__blocker"></div>
      </div>
      <!-- slider CAPTIONS -->
      <div class="slider-images__captions swiper-container js-slider-images__captions mt-small" data-slides-per-view="3"
        data-slides-per-view-tablet="1.5" data-slides-per-view-mobile="1.25" data-centered-slides="true"
        data-centered-slides-tablet="true" data-centered-slides-mobile="true" data-arts-os-animation="true"
        data-arts-os-animation-name="animateJump">
        <div class="swiper-wrapper">
          {% for post in collections.realizacje %}
          <!-- caption -->
          <div class="swiper-slide pt-3 js-arts-cursor-highlight">
            <div class="slider-images__caption h6">01&nbsp;&nbsp;/&nbsp;&nbsp;{{ post.data.title }}</div>
          </div>
          {% endfor %}
          <div class="slider-images__underline js-slider-images__underline"></div>
        </div>
      </div>
      <!-- - slider CAPTIONS -->
      <!-- slider ARROWS -->
      <div class="slider__arrow slider__arrow_absolute slider__arrow_left js-slider__arrow-prev d-none d-md-block"
        data-arts-os-animation="true" data-arts-os-animation-name="animateJump">
        <div class="slider__arrow material-icons js-slider__arrow-prev">keyboard_arrow_left</div>
      </div>
      <div class="slider__arrow slider__arrow_absolute slider__arrow_right js-slider__arrow-next d-none d-md-block"
        data-arts-os-animation="true" data-arts-os-animation-name="animateJump">
        <div class="slider__arrow material-icons js-slider__arrow-next">keyboard_arrow_right</div>
      </div>
      <!-- - slider ARROWS -->
    </div>
  </div>
</section>