{% include "partials/header.njk" %}

<section class="section section-masthead text-center pt-xlarge bg-light-1" data-arts-os-animation="true">
    <div class="container">
        <h1 class="xl section-masthead__heading d-inline-block my-0 arts-split-text js-arts-split-text js-transition-heading"
            data-arts-split-text="lines" data-arts-split-text-set="lines"
            data-arts-split-text-overflow-wrap="lines">{{ client }}</h1>
        <div class="section-masthead__subheading mt-3 arts-split-text js-arts-split-text"
            data-arts-split-text="lines" data-arts-split-text-set="lines"
            data-arts-split-text-overflow-wrap="lines">
            <ul class="post-meta post-meta_mini">
                <li><a href="#">{{ category }}</a>
                    <div class="post-meta__divider section-masthead__meta-divider"></div>
                </li>
                <li><a href="#">2021</a></li>
            </ul>
        </div>
    </div>
    <div class="section-masthead__wrapper-content mt-5">
        <div class="section__content">
            <div class="container">
                <div class="row">
                    <div class="col">
                        <div class="mask-reveal js-transition-img js-arts-parallax overflow"
                            data-arts-parallax-factor-y="0.05">
                            <div class="mask-reveal__layer mask-reveal__layer-1">
                                <div class="mask-reveal__layer mask-reveal__layer-2">
                                    <div class="js-arts-parallax__scrub js-transition-img__transformed-el"><img
                                            src="{{ thumbnail | url }}" width="1920" height="1280" alt="alt" /></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="section__bg top-offset top-offset_large js-transition-bg bg-light-2"
            data-background-color="var(--color-light-2)"></div>
    </div>
</section>

